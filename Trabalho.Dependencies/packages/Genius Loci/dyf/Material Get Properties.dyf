      <Script>#Alban de Chasteigner 2018
#twitter : @geniusloci_bim
#geniusloci.bim@gmail.com

import clr
clr.AddReference('RevitAPI')
from Autodesk.Revit.DB import *

clr.AddReference('DSCoreNodes')
import DSCore
from DSCore import Color

clr.AddReference('RevitServices')
import RevitServices
from RevitServices.Persistence import DocumentManager
doc = DocumentManager.Instance.CurrentDBDocument
uiapp = DocumentManager.Instance.CurrentUIApplication
app = uiapp.Application
version=int(app.VersionNumber)

materials = UnwrapElement(IN[0]) if isinstance(IN[0],list) else [UnwrapElement(IN[0])]
colorname,sforpatternlist,sbacpatternlist,cforpatternlist,cbacpatternlist,transparencylist,rgbList,rgbSurforList,rgbSurbacList,rgbCutforList,rgbCutbacList=[],[],[],[],[],[],[],[],[],[],[]

for material in materials :
	colorname.append(material.Name)
	
	if material.Color.IsValid:
		color=material.Color
		rgb = DSCore.Color.ByARGB(255, color.Red, color.Green, color.Blue)
	else:
		rgb = None
	rgbList.append(rgb)
	
	transparencylist.append(material.Transparency)
	
	if version &lt; 2019 :
		sforpatternlist.append(doc.GetElement(material.SurfacePatternId))
		surcolor = material.SurfacePatternColor
		rgbSf = DSCore.Color.ByARGB(255, surcolor.Red, surcolor.Green, surcolor.Blue)
		rgbSurforList.Add(rgbSf)
		sbacpatternlist.append(doc.GetElement(material.SurfacePatternId))
		surbaccolor=material.SurfacePatternColor
		rgbSb = DSCore.Color.ByARGB(255, surbaccolor.Red, surbaccolor.Green, surbaccolor.Blue)
		rgbSurbacList.Add(rgbSb)
		
		cforpatternlist.append(doc.GetElement(material.CutPatternId))
		cutcolor=material.CutPatternColor
		rgbCf = DSCore.Color.ByARGB(255, cutcolor.Red, cutcolor.Green, cutcolor.Blue)
		rgbCutforList.Add(rgbCf)
		cbacpatternlist.append(doc.GetElement(material.CutPatternId))
		cutbaccolor = material.CutPatternColor
		rgbCb = DSCore.Color.ByARGB(255, cutbaccolor.Red, cutbaccolor.Green, cutbaccolor.Blue)
		rgbCutbacList.Add(rgbCb)
		
	else:
		sforpatternlist.append(doc.GetElement(material.SurfaceForegroundPatternId))
		surcolor = material.SurfaceForegroundPatternColor
		rgbSf = DSCore.Color.ByARGB(255, surcolor.Red, surcolor.Green, surcolor.Blue)
		rgbSurforList.Add(rgbSf)
	
		sbacpatternlist.append(doc.GetElement(material.SurfaceBackgroundPatternId))
		surbaccolor=material.SurfaceBackgroundPatternColor
		rgbSb = DSCore.Color.ByARGB(255, surbaccolor.Red, surbaccolor.Green, surbaccolor.Blue)
		rgbSurbacList.Add(rgbSb)	
	
		cforpatternlist.append(doc.GetElement(material.CutForegroundPatternId))
		cutcolor=material.CutForegroundPatternColor
		rgbCf = DSCore.Color.ByARGB(255, cutcolor.Red, cutcolor.Green, cutcolor.Blue)
		rgbCutforList.Add(rgbCf)
	
		cbacpatternlist.append(doc.GetElement(material.CutBackgroundPatternId))
		cutbaccolor = material.CutBackgroundPatternColor
		rgbCb = DSCore.Color.ByARGB(255, cutbaccolor.Red, cutbaccolor.Green, cutbaccolor.Blue)
		rgbCutbacList.Add(rgbCb)
	
OUT = colorname,rgbList,sforpatternlist,rgbSurforList,sbacpatternlist,rgbSurbacList,cforpatternlist,rgbCutforList,cbacpatternlist,rgbCutbacList,transparencylist</Script>
